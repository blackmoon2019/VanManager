    <DataSet Name="POI">
      <Query>
        <DataSourceName>Van</DataSourceName>
        <CommandText>select ardelt,R.stiname ,P.ROUTEID
from INVD 
INNER JOIN vw_POI P ON P.id = INVD.poiId 
inner join vw_ROUTES R on P.routeID = R.ID 
order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
